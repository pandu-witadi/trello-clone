{
    "host": "localhost",
    "port": 3030,
    "public": "../public/",
    "paginate": {
        "default": 100,
        "max": 500
    },
    "mongodb": "mongodb://localhost:27017/trello-clone",
    "authentication": {
        "secret": "",
        "strategies": [
            "jwt",
            "local"
        ],
        "path": "/authentication",
        "service": "users",
        "jwt": {
            "header": {
                "typ": "access"
            },
            "audience": "https://yourdomain.com",
            "subject": "anonymous",
            "issuer": "feathers",
            "algorithm": "HS256",
            "expiresIn": "1d"
        },
        "local": {
            "entity": "user",
            "usernameField": "email",
            "passwordField": "password"
        }
    }
}
